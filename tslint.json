{
  "extends": [
    "tslint-config-airbnb"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier",
    "codelyzer"
  ],
  "rules": {
    "prettier": [
      true,
      {
        "singleQuote": true,
        "trailingComma": "es5",
        "printWidth": 100
      }
    ],
    "ordered-imports": true,
    "trailing-comma": "es5",
    "interface-name": false,
    "ter-arrow-parens": [
      true,
      "as-needed"
    ],
    "function-name": false,
    "variable-name": false,
    "align": false,
    "semicolon": false,
    "brace-style": false
  }
}